@model IEnumerable<ASPNET5VCsTest1.Models.Employee>

<h2> ASP.NET MVC 6 View Component Test.</h2>
<h4>@ViewBag.Message</h4>
<table border="1">
    <tr>
        <td>Emp ID</td>
        <td>Emp Name</td>
    </tr>
@foreach (var emp in Model)
{
    <tr>
            <td>@emp.ID</td>
            <td>@emp.EmpName</td>
    </tr>
}
</table>